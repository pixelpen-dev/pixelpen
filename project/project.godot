; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="PixelPen"
config/version="1.1.4"
run/main_scene="res://addons/net.yarvis.pixel_pen/editor/editor_main_ui.tscn"
config/features=PackedStringArray("4.3", "Mobile")
boot_splash/show_image=false
config/icon="res://addons/net.yarvis.pixel_pen/resources/icon/Icon.png"

[display]

window/size/viewport_width=1280
window/size/viewport_height=720
window/size/mode=2

[editor_plugins]

enabled=PackedStringArray("res://addons/net.yarvis.pixel_pen/plugin.cfg")

[input_devices]

pen_tablet/driver.windows="wintab"

[rendering]

renderer/rendering_method="mobile"
textures/vram_compression/import_etc2_astc=true
